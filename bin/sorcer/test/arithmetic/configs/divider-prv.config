/* 
* Provider dependency injections
* It uses component entry names defined in the SORCER provider
* as defined by sorcer.core.provider.SorcerProvider.
*/
import net.jini.jrmp.*;
import net.jini.jeri.*;
import net.jini.jeri.tcp.*;
import sorcer.core.*;
import net.jini.core.entry.Entry;
import net.jini.lookup.entry.*;
import sorcer.core.provider.*;
import sorcer.arithmetic.provider.*;

sorcer.core.provider.ServiceExerter {
        /* service provider generic properties */
		name = "Divider";
		description = "Divider - bean provider";
		location = "AFRL/WPAFB";
		publishedInterfaces = new Class[] { junit.sorcer.core.provider.Divider.class };

		spaceEnabled = true;
		
		// persist and reuse service ID
		providerIdPersistent = false;
		
		// service beans	
		beanClasses = new Class[] { junit.sorcer.core.provider.DividerImpl.class }; 
		
		iconName="sorcer.jpg";
}
	
sorcer.core.exertion.deployment {
    interfaces = new String[]{"junit.sorcer.core.provider.Divider"};
    codebaseJars = new String[]{"ju-arithmetic-dl.jar"};
    implJars = new String[]{"ju-arithmetic-beans.jar"};
}


